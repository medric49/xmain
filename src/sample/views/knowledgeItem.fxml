<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane fx:id="pane" minHeight="0.0" minWidth="0.0" prefHeight="51.0" prefWidth="130.0" styleClass="knowledge-item-container" stylesheets="@components.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.components.KnowledgeItemController">
   <children>
      <VBox fx:id="vbox" prefHeight="44.0" prefWidth="171.0">
         <children>
            <StackPane alignment="CENTER_LEFT" prefHeight="150.0" prefWidth="200.0">
               <children>
                  <Label fx:id="title" text="Label" textFill="WHITE" />
               </children>
            </StackPane>
            <StackPane alignment="CENTER_RIGHT" prefHeight="150.0" prefWidth="200.0">
               <children>
                  <Label fx:id="details" text="Label" textFill="WHITE" />
               </children>
            </StackPane>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</StackPane>
